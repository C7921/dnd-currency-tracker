{
  "name": "dnd-currency-tracker",
  "version": "1.0.0",
  "description": "A simple D&D 5e currency tracker",
  "main": "app.js",
  "scripts": {
    "start": "node app.js",
    "dev": "nodemon app.js",
    "docker:dev": "docker-compose up -d",
    "docker:logs": "docker-compose logs -f",
    "docker:stop": "docker-compose down",
    "seed": "node scripts/seed.js"
  },
  "engines": {
    "node": "18.x"
  },
  "dependencies": {
    "express": "4.17.1",
    "mongoose": "6.0.12"
  },
  "devDependencies": {
    "nodemon": "2.0.22"
  }
}
